{
    "name": "abdullmng/laravel-distance",
    "description": "Calculate distance between addresses or coordinates with built-in geocoding",
    "keywords": [
        "laravel",
        "distance",
        "geolocation",
        "geocoding",
        "logistics",
        "addresses"
    ],
    "version": "1.0.2",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Muhammad Abdullahi",
            "email": "abdullmng@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2|^8.3|^8.4",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Abdullmng\\Distance\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Abdullmng\\Distance\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Abdullmng\\Distance\\DistanceServiceProvider"
            ],
            "aliases": {
                "Distance": "Abdullmng\\Distance\\Facades\\Distance"
            }
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
